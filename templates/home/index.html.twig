{% extends 'layout.html.twig' %}

{% block title %}HomePage{% endblock %}

{% block body %}
    {% for message in app.flashes('notice') %}
        <div class="flash-notice">
            {{ message }}
        </div>
    {% endfor %}
    {% if app.user %}
        <div class="mb-3">
            You are logged in as {{ app.user.username }}, <a href="{{ path('app_logout') }}">Logout</a>
        </div>
    {% endif %}
    <header class="masthead" style="background-image:url('assets/img/imgprojet6.jpg');">
        <div class="container">
            <div class="intro-text">
                <div class="intro-lead-in"></div>
                <div class="intro-heading text-uppercase">
                    <span></span></div>
                <br>
                <a class="btn btn-primary btn-xl text-uppercase" role="button" href="{{ path('Reservation') }}" style="background: rgb(0,28,15);border-top-color: rgb(0,28,15);border-right-color: rgb(0,28,15);font-family: 'Kaushan Script';border-bottom-color: rgb(0,28,15);border-left-color: rgb(0,28,15);text-shadow: 0px 0px;margin-top: 300px;margin-left: 100px">Réservez!</a>
              <br><br><br>  <h4 style="color: rgb(2,33,7);font-family: 'Kaushan Script', serif;font-size: 41.4px;margin-left: 100px">Bienvenu chez nous !</h4>
            </div>
        </div>
    </header>
    <section id="services">
        <section id="about" style="margin-top: -153px;background: #f5ffff;">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h4 style="color: rgb(0,92,15);font-family: 'Kaushan Script', serif;font-size: 41.4px;">Pourquoi nous?<br><br></h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="list-group timeline">
                            <li class="list-group-item">
                                <div class="timeline-image"><img class="rounded-circle img-fluid" src="assets/img/about/image.png"></div>
                                <div class="timeline-panel">
                                    <div class="timeline-heading">
                                        <br><br>
                                        <h4 class="subheading">Un projet à présenter?</h4>
                                    </div>
                                </div>
                            </li>
                            <li class="list-group-item timeline-inverted">
                                <div class="timeline-image"><img class="rounded-circle img-fluid" src="assets/img/about/image2.png"></div>
                                <div class="timeline-panel">
                                    <div class="timeline-heading">
                                        <br><br>
                                        <h4 class="subheading">Un travail en groupe?</h4>
                                    </div>
                                </div>
                            </li>
                            <li class="list-group-item">
                                <div class="timeline-image"><img class="rounded-circle img-fluid" src="assets/img/about/3.jpg"></div>
                                <div class="timeline-panel">
                                    <div class="timeline-heading">
                                        <br><br>
                                        <h4 class="subheading">Une conférence à organiser?</h4>
                                    </div>
                                </div>
                            </li>
                            <li class="list-group-item timeline-inverted">
                                <div class="timeline-image"><img class="rounded-circle img-fluid" src="assets/img/about/image3.png"></div>
                                <div class="timeline-panel">
                                    <div class="timeline-heading">
                                        <br><br>
                                        <h4 class="subheading">Un local pour votre formation?</h4>
                                    </div>
                                </div>
                            </li>
                            <li class="list-group-item timeline-inverted">
                                <div class="timeline-image" style="background: rgb(0,92,15);border-top-color: rgba(233,236,239,0);border-right-color: rgba(233,236,239,0);border-bottom-color: rgba(233,236,239,0);border-left-color: rgba(233,236,239,0);">
                                    <h4>Be Part<br>&nbsp;Of Our<br>&nbsp;Story!</h4>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

<br><br>
            <h4 style="color: rgb(0,92,15);font-family: 'Kaushan Script', serif;font-size: 35.4px;text-align: center;">Notre galerie photo <br /><br /></h4>
            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="assets/img/imagesofienee.png" alt="First slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="assets/img/imgprojet5.jpg" alt="Second slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="assets/img/imgprojet6.jpg" alt="Third slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="assets/img/imgprojet7 - Copie.jpg" alt="Third slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="assets/img/imagemaryem.png" alt="Third slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="assets/img/imgprojet8 - Copie.jpg" alt="Third slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="assets/img/imagee.png" alt="Third slide">
                    </div>
                </div>
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>

            <section class="features-boxed" style="margin-top: -152px;background: #f5ffff;">
                <div class="container">
                    <div class="intro">
                        <h2 class="text-center" style="color: rgb(0,92,15);font-family: 'Kaushan Script', serif;font-size: 42px;">Nous offrons</h2>
                        <br>
                        <br>
                    </div>
                    <div class="row justify-content-center features">
                        <div class="col-sm-6 col-md-5 col-lg-4 item">
                            <div class="box"><i class="fa fa-star icon" style="color: rgb(0,92,15);"></i>
                                <h3 class="name">Qualité</h3>
                                <p class="description">Une bonne qualité n'est pas une option,<br> c'est une obligation</p>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-5 col-lg-4 item">
                            <div class="box"><i class="fa fa-sun-o icon" style="color: rgb(0,92,15);"></i>
                                <h3 class="name">Flexibité</h3>
                                <p class="description">Notre manière de vous servir.</p>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-5 col-lg-4 item">
                            <div class="box"><i class="fa fa-internet-explorer icon" style="color: rgb(0,92,15);"></i>
                                <h3 class="name">Internet </h3>
                                <p class="description">Profitez d'un haut debit</p>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-5 col-lg-4 item">
                            <div class="box"><i class="fa fa-car icon" style="color: rgb(0,92,15);"></i>
                                <h3 class="name">Stationnement</h3>
                                <p class="description">Un immence parking s'offre à vous.</p>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-5 col-lg-4 item">
                            <div class="box"><i class="fa fa-plus icon" style="color: rgb(0,92,15);"></i>
                                <h3 class="name">Salles de conférences </h3>
                                <p class="description">Le parfait endroit pour organiser vos conférences.</p>
                            </div>
                        </div>
                        <div class="col-sm-6 col-md-5 col-lg-4 item">
                            <div class="box"><i class="fa fa-phone icon" style="color: rgb(0,92,15);"></i>
                                <h3 class="name">Disponibilité</h3>
                                <p class="description">24h/24 à votre disposition.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </section>
        <section id="about" style="margin-top: -179px;background: #f5ffff;">
            <div class="simple-slider">
                <div class="swiper-container">
                    <div class="swiper-wrapper"></div>
                    <div class="swiper-pagination"></div>
                    <div class="swiper-button-prev"></div>
                    <div class="swiper-button-next"></div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <ul class="list-group timeline"></ul>
                    </div>
                </div>
            </div>
        </section>
    </section>
    <section class="py-5" style="background: #f5ffff;">
        <section class="map-clean" style="background: #f5ffff;">
            <div class="container" style="margin-top: -597px;">
                <div class="intro">
                    <h2 class="text-center" style="color: rgb(1,128,29);font-family: 'Kaushan Script', serif;font-size: 39.1px;">Rejoignez nous !</h2>
                </div>
            </div>
            {% include '/map/index.html.twig' %}
        </section>
    </section>
{% endblock %}



